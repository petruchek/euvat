{
  "name": "petruchek/euvat",
  "description": "Modified PHP library for dealing with EU VAT",
  "keywords": ["vat"],
  "license": "MIT",
  "authors": [
    {
      "name": "Danny van Kooten",
      "email": "hi@dvk.co"
    },
    {
      "name": "Val Petruchek",
      "email": "petruchek@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Petruchek\\EuVat\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true,
  "suggest": {
    "ext-soap": "Needed to support VIES VAT number validation"
  }
}
